{
  "name": "justjshtml",
  "version": "0.0.1",
  "description": "A JavaScript port of the justhtml library.",
  "main": "dist/justjshtml.cjs",
  "module": "dist/justjshtml.mjs",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/justjshtml.mjs",
      "require": "./dist/justjshtml.cjs"
    }
  },
  "scripts": {
    "test": "node tests/parser.test.js",
    "build": "rollup -c && cpx playground.html dist && cpx src/index.js dist/src",
    "prebuild": "rm -rf dist"
  },
  "keywords": [
    "html",
    "parser"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup": "^4.19.0",
    "cpx": "^1.5.0"
  }
}